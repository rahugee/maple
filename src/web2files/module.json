{
  "name": "maple",
  "maple/web2": {
    "extra": [
      "page/view.story.js"
    ],
    "js": [
      "maple.app.js"
    ],
    "on": ["maple/webmodules", "maple/spamjs", "maple/home"]
  },
  "maple/home": {
    "js": [
      "webinfo/maple.statbox.js",
      "searchbar/maple.searchbar.js",
      "storieslist/maple.storieslist.js",
      "storieslist/maple.storieslist.search.js",
      "home/maple.home.js",
      "story/maple.story.js",
      "author/maple.author.js",
      "actors/maple.actor.search.js",
      "actors/maple.actor.profile.js"
    ],
    "html" : [
      "webinfo/catpanal.html", "webinfo/statbox.html", "topbar/topbar.html","searchbar/searchbar.html",
      "searchbar/searchbar.html","home/home.html","storieslist/storieslist.html","storieslist/search_result.html"
    ],
    "on": ["maple/webmodules",  "maple/spamjs",  "maple/service" ]
  },
  "maple/stories/html": {
    "html" : [
      "story/chapter_comment.html",
      "story/chapter_discuss.html",
      "story/chapter_read.html",
      "story/story.html",
      "story/story_details.html"
    ]
  },
  "maple/style": {
    "css": [
      "../external/bootstrap/css/bootstrap.min.css",
    "../../dist/style/library.css","../external/components/font-awesome/css/font-awesome.min.css",
    "../maple.css"
    ]
  },
  "maple/service": {
    "js": [
      "service/DataService.js",
      "maple.webinfo.js"
    ],
    "on": [  "jsutils/cache" ]
  }
}